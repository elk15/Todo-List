{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
	    "indent": ["error", 4],
	    "max-classes-per-file": ["error", 3],
        "prefer-const" : "off"    
    }
}